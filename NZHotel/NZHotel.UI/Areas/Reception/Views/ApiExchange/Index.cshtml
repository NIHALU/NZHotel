@model IEnumerable<ExchangeRatesModel2.Exchange_Rates>

	<h3 class="bg-dark mt-3 text-white text-center">Currency Rates ($)</h3>
	<div class="container-fluid">
  
		<table id="myTable" class="mt-3 table table-sm table-hover table-striped">
	        <thead>
				<tr>
				   <th>Currency Name</th>
				   <th>Currency Rate</th>
				
				</tr>
			</thead>

			<tbody>
			@foreach(var item in Model)
				{
				<tr>
				   <td> @item.currency</td>
				   <td> @item.exchange_rate_buy</td>

				</tr>
				}
			</tbody>

		</table>
	

</div>








