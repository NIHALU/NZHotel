@model List<GuestReservationListDto>

<h3 class="bg-dark my-3 text-white text-center">Past Reservation List</h3>
<div class="container-fluid">
	@{
		if (Model?.Count >= 0)
		{
			<table id="myTable" class="mt-3 table table-sm table-hover table-striped">
				<thead>
					<tr>
						<th>Id</th>
						<th>G-FullName</th>
						<th>R-Code</th>
						<th>R-Starting Date</th>
						<th>R-Finishing Date </th>
						<th>R-Total Amount</th>
						<th>R-Room No</th>
						<th>R-Room Name</th>
						<th>Check-In Time </th>
						<th>Check-Out Time</th>
						
					


					</tr>
				</thead>

				<tbody>
					@foreach (var guestRes in Model)
					{
						<tr>
							<td>@guestRes.Id</td>
							<td> @(guestRes.Guest.Name+" "+@guestRes.Guest.Surname)</td>
							<td> @guestRes.Reservation.ReservationCode</td>
							<td> @guestRes.Reservation.StartingDate</td>
							<td> @guestRes.Reservation.FinishingDate</td>
							<td> @guestRes.Reservation.TotalAmount</td>
							<td> @guestRes.Reservation.Room.RoomNo </td>
							<td> @guestRes.Reservation.Room.RoomName </td>
							
							<td> @guestRes.CheckInTime</td>
							<td> @guestRes.CheckOutTime</td>

					


						</tr>
					}
				</tbody>

			</table>
		}
		else
		{
			<div class="text-center my-3 p-1">
				There is No Guest Data For Now.
			</div>
		}
	}
</div>
